{

  "manifest_version": 2,
  "name": "Redirector",
  "version": "1.0",

  "description": "Redirects all searches from bing to google.",

  "icons": {
    "48": "icons/anti-bing.png"
  },
  
  "browser_specific_settings": {
    "gecko": {
      "id": "extension@xScruffers.github.com"
    }
  }

  "content_scripts": [
    {
      "matches": ["*://*.bing.com/*"],
      "js": ["redirecter.js"]
    }
  ]

}
